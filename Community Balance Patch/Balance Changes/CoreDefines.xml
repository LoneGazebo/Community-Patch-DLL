<GameData>
	<Table name="Yield_Flavors">
		<Column name="YieldType" type="text" reference="Yields(Type)"/>
		<Column name="FlavorType" type="text" reference="Flavors(Type)"/>
		<Column name="Flavor" type="integer" notnull="true"/>
	</Table>
	<AIGrandStrategy_Flavors>
		<Update>
			<Where AIGrandStrategyType ="AIGRANDSTRATEGY_CONQUEST"/>
			<Set FlavorType="FLAVOR_OFFENSE"
			Flavor="20"/>
		</Update>
		<Update>
			<Where AIGrandStrategyType ="AIGRANDSTRATEGY_CULTURE"/>
			<Set FlavorType="FLAVOR_CULTURE"
			Flavor="20"/>
		</Update>
		<Update>
			<Where AIGrandStrategyType ="AIGRANDSTRATEGY_UNITED_NATIONS"/>
			<Set FlavorType="FLAVOR_DIPLOMACY"
			Flavor="20"/>
		</Update>
		<Update>
			<Where AIGrandStrategyType ="AIGRANDSTRATEGY_SPACESHIP"/>
			<Set FlavorType="FLAVOR_SPACESHIP"
			Flavor="20"/>
		</Update>
	</AIGrandStrategy_Flavors>
