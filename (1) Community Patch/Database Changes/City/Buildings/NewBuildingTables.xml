	<Table name="Building_YieldFromYieldPercent">
		<Column name="BuildingType" type="text" reference="Buildings(Type)"/>
		<Column name="YieldOut" type="text" reference="Yields(Type)"/>
		<Column name="YieldIn" type="text" reference="Yields(Type)"/>
